<%- include ('../../../partials/commons/head')%>
  <title>Admon: Detalle de Subcategoría</title>
  <link rel="stylesheet" href="/css/productsstyles.css">
  </head>
  <%- include ('../../../partials/commons/header')%>
    <main class="container-fluid">
      <!--Detalle de Producto-->
      <br><br>
      <section class="row">
        <article class="col-12  card">
          <div class="card-body">
            <h2 class="display-4 text-primary  text-center pt-2">Detalle de la Subcategoría</h2>
          </div>
        </article>
        <article class="col-12 col-md-6 mx-auto mt-5 producto">

          <div class="card-body">
            <h5 class="card-title">
              <%= subcategory.name%>
            </h5>
            <p class="card-text">
              <%= subcategory.description %>
            </p>
            <div class="subitem">
              <figure class="form-group">
                <img width="25%" class="w-100" src="/db-images/products/subcategories/<%=subcategory.image%>" alt="services-others"
                  <%=subcategory.image%>>
              </figure>
            </div>
          </div>
          <a href="/admin/products/subcategories" class="btn btn-danger">Volver</a>
        </article>
      </section>
    </main>
    <%- include ('../../../partials/commons/footer')%>