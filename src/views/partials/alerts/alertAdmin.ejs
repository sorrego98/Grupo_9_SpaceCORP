<div class="show-alerts">
    <% if (locals.cookie && locals.cookie.responseData) { %>
      <% locals.cookie.responseData.forEach(msg => { %>
        <div class="alert <%= msg.success ? 'bg-success' : 'bg-alert' %>">
          <div class="contain-icon <%= msg.success ? 'success-icon' : 'err-icon' %>">
            <i class="fa-solid <%= msg.success ? 'fa-circle-check' : 'fa-circle-xmark' %>"></i>
          </div>
          <div class="msg-show">
            <div>
              <span class="highlight <%= msg.success ? 'success-text' : 'err-text' %>">Mensaje:</span> <p class="<%= msg.success ? 'success-text' : 'err-text' %>"><%= msg.message %></p>
            </div>
          </div>
        </div>
      <% }) %>
    <% } %>
  </div>